                                "cfn-signal -e 1",
                                "   --stack ",
                                {
                                    "Ref": "AWS::StackName"
                                },
                                "   --resource AnsibleConfigServer ",
                                "   --region ",
                                {
                                    "Ref": "AWS::Region"
                                },
                                "\n",
                                "echo \" \t#################[] End of else block \" \n",
                                "fi\n"
                            ]
                        ]
