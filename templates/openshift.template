                                "aws s3 cp ${SCRIPT_PATH}scripts/scaling/setup.py /root/ose_scaling/setup.py \n",
                                "pip install /root/ose_scaling \n",
                                "# Start cfn-configset [cfg_node_keys]\n",
                                "cfn-init -v ",
                                "--stack ",
                                {
                                    "Ref": "AWS::StackName"
                                },
                                " --resource AnsibleConfigServer",
