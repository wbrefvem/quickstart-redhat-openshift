                    "detail-type": [
                        "EC2 Instance Launch Successful",
                        "EC2 Instance Terminate Successful",
                        "EC2 Instance Terminate Unsuccessful"
                    ],
                    "detail": {
                        "AutoScalingGroupName": [
                            {
                                "Ref": "OpenShiftMasterASG"
                            },
                            {
                                "Ref": "OpenShiftEtcdASG"
                            },
                            {
                                "Ref": "OpenShiftNodeASG"
                            }
                        ]
                    }
                },
                "Targets": [
