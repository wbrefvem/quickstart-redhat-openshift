                                {
                                    "Ref": "AWS::StackName"
                                },
                                "   --resource AnsibleConfigServer ",
                                "   --configsets cfg_ansible ",
                                "   --region ",
                                {
                                    "Ref": "AWS::Region"
                                },
                                "\n",
